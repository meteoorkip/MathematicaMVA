3ebcd5039755"],

Cell[TextData[Cell[BoxData[{
 FormBox[
  StyleBox[
   RowBox[{
    RowBox[{"E", "[", 
     RowBox[{
      SubscriptBox["N", "a"], "(", "K", ")"}], "]"}], "=", 
    FractionBox[
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}], "K"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"K", "-", "i"}], ")"}], 
        SuperscriptBox[
         SubscriptBox["D", "a"], 
         RowBox[{"K", "-", "1"}]], 
        SuperscriptBox[
         SubscriptBox["D", "b"], "i"]}], ")"}]}], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"j", "=", "0"}], "K"], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["D", "a"], 
         RowBox[{"K", "-", "j"}]], 
        SuperscriptBox[
         SubscriptBox["D", "b"], 
         RowBox[{"^", "j"}]]}], ")"}]}]]}],
   FontFamily->"Consolas",
   FontSlant->"Italic"], TraditionalForm], "\[IndentingNewLine]", 
 FormBox[
  StyleBox[
   RowBox[{
    RowBox[{"E", "[", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["R", "^"], "a"], "(", "K", ")"}], "]"}], "=", 
    FractionBox[
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "0"}], "K"], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"K", "-", "i"}], ")"}], 
        SuperscriptBox[
         SubscriptBox["D", "a"], 
         RowBox[{"K", "-", "i"}]], 
        SuperscriptBox[
         SubscriptBox["D", "b"], "i"]}], ")"}]}], 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"j", "=", "0"}], 
       RowBox[{"K", "-", "1"}]], 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["D", "a"], 
         RowBox[{"K", "-", "1", "-", "j"}]], 
        SuperscriptBox[
         SubscriptBox["D", "b"], "j"]}], ")"}]}]]}],
